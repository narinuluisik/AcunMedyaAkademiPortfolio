
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@Html.Action("PartialHead", "Default")
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">

    @Html.Action("PartialNavbar", "Default")

    @Html.Action("PartialFeature", "Default")
    @Html.Action("PartialStatistic", "Default")



    <section class="ftco-about ftco-section ftco-no-pt ftco-no-pb" id="about-section">
        <div class="container">
            <div class="row d-flex no-gutters">
                <div class="col-md-6 col-lg-5 d-flex">
                    <div class="img-about img d-flex align-items-stretch">
                        <div class="overlay"></div>
                        <div class="img d-flex align-self-stretch align-items-center" style="background-image: url(https://i.pinimg.com/originals/f5/36/01/f53601133f236d1cb167ac19f05a3d60.gif); ">
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-7 pl-md-4 pl-lg-5 py-5">
                    <div class="py-md-5">
                        <div class="row justify-content-start pb-3">
                            <div class="col-md-12 heading-section ftco-animate">
                                @Html.Action("PartialAbout", "Default")
                                @Html.Action("PartialAdress", "Default")

                            </div>
                            @Html.Action("PartialHobby", "Default")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    @Html.Action("PartialSkill", "Default")
    @Html.Action("PartialService", "Default")


    <section class="ftco-hireme">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-md-8 col-lg-8 d-flex align-items-center">
                    <div class="w-100 py-4">
                        <h2>Bir projeniz mi var?</h2>
                        <p>Projeniz için en iyi çözümleri sunmak adına yanınızdayız. Her adımda kaliteyi ve yenilikçiliği ön planda tutarak, hayalinizdeki projeyi gerçeğe dönüştürmek için çalışıyoruz.</p>

                        <p class="mb-0"><a href="#" class="btn btn-white py-3 px-4">Benimle iletişime geçin</a></p>
                    </div>
                </div>
                <div class="col-md-4 col-lg-4 d-flex align-items-end">
                    <img src="images/author.png" class="img-fluid" alt="">
                </div>
            </div>
        </div>
    </section>



    @Html.Action("PartialProject", "Default")
    @Html.Action("PartialTestimonial", "Default")



    @Html.Action("PartialContact", "Default")





    @Html.Action("PartialFooterService", "Default")
    @Html.Action("PartialFooterAddress", "Default")



    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>




    <script src="~/Template/clyde-master/js/jquery.min.js"></script>
    <script src="~/Template/clyde-master/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/Template/clyde-master/js/popper.min.js"></script>
    <script src="~/Template/clyde-master/js/bootstrap.min.js"></script>
    <script src="~/Template/clyde-master/js/jquery.easing.1.3.js"></script>
    <script src="~/Template/clyde-master/js/jquery.waypoints.min.js"></script>
    <script src="~/Template/clyde-master/js/jquery.stellar.min.js"></script>
    <script src="~/Template/clyde-master/js/owl.carousel.min.js"></script>
    <script src="~/Template/clyde-master/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Template/clyde-master/js/jquery.animateNumber.min.js"></script>
    <script src="~/Template/clyde-master/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="~/Template/clyde-master/js/google-map.js"></script>

    <script src="~/Template/clyde-master/js/main.js"></script>

</body>
</html>