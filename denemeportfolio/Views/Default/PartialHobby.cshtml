@model List<denemeportfolio.Models.TblHobby>

<div class="col-md-12">
    <div class="my-interest d-lg-flex w-100">
        @foreach (var item in Model)
        {
            <div class="interest-wrap d-flex align-items-center">
                <div class="icon d-flex align-items-center justify-content-center">
                    <img src="@item.IconUrl" alt="@item.Title" style="width: 50px; height: 50px; object-fit: contain; border-radius: 50%; margin: 5px;" />
                </div>
                <div class="text">@item.Title</div>
            </div>
        }
    </div>
</div>
