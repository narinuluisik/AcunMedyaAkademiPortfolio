@model denemeportfolio.Models.TblHobby
@{
    ViewBag.Title = "CreateHobby";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Hobby Ekleme</h2>

<form method="post">
 
    <label>Hobyy ismi</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })

    <label>Hobby Resmi</label>
    @Html.TextBoxFor(x => x.IconUrl, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Referansı kaydet</button>
</form>


